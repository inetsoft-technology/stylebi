        </pluginManagement>
        <plugins>
          <plugin>
            <groupId>com.mycila</groupId>
            <artifactId>license-maven-plugin</artifactId>
            <configuration>
              <properties>
                <owner>InetSoft Technology</owner>
                <email>info@inetsoft.com</email>
                <year>2024</year>
              </properties>
              <licenseSets>
                <licenseSet>
                  <header>com/mycila/maven/plugin/license/templates/AGPL-3.txt</header>
                </licenseSet>
              </licenseSets>
              <mapping>
                <java>SLASHSTAR_STYLE</java>
                <ts>SLASHSTAR_STYLE</ts>
              </mapping>
            </configuration>
          </plugin>
        </plugins>
      </build>
    </profile>
